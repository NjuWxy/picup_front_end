{
  "entry": "src/index.js",
    "proxy": {
      "/api": {
        "target": "http://localhost:8080/",
        "changeOrigin": true,
        "pathRewrite": { "^/api" : "" }
      },
      "/image": {
         "target": "http://localhost:8080/",
         "changeOrigin": true,
         "pathRewrite": { "^/image" : " " }
      }
    },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
